package oopscape;

public class MultiThreading implements Runnable{
	private Thread t;
	private String arr;
	private int index;
	private String threadName;
	MultiThreading(String arr,String threadName){
		this.arr=arr;
		this.index=index;
		this.threadName=threadName;
	}
	@Override
	public void run() {
		try{
				System.out.println(threadName);
				Thread.sleep(100);
			
		}
		catch(Exception e){
			e.printStackTrace();
		}
	}
	public void start(){
		if(t==null){
			t= new Thread(this,"thread1");
			t.start();
		}
	}

}
